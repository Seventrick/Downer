[dialogic]

directories/dch_directory={
"athletic_dude": "res://dialogic/characters/athletic_dude.dch",
"dialogueTestCharacter": "res://dialogic/characters/dialogueTestCharacter.dch",
"downer": "res://dialogic/characters/downer.dch",
"mom": "res://dialogic/characters/mom.dch",
"nick": "res://dialogic/characters/nick.dch",
"preDowner": "res://dialogic/characters/preDowner.dch",
"skinny_girl": "res://dialogic/characters/skinny_girl.dch",
"testCharacter": "res://dialogic/testCharacter.dch"
}
directories/dtl_directory={
"Skinny_Girl": "res://dialogic/timelines/Skinny_Girl.dtl",
"TestTimeline": "res://dialogic/TestTimeline.dtl",
"athletic_dude": "res://dialogic/timelines/athletic_dude.dtl",
"daytimeTestTimeline": "res://dialogic/timelines/daytimeTestTimeline.dtl",
"dialogueTestTimeline": "res://dialogic/timelines/dialogueTestTimeline.dtl",
"downerIntro": "res://dialogic/timelines/downerIntro.dtl",
"nick_mom": "res://dialogic/timelines/nick_mom.dtl",
